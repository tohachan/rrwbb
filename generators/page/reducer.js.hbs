const defaultState = {
    data: ''
};

const initialState = (typeof window !== 'undefined')
    ? window.__INITIAL__DATA__.{{lowerCase name}} || defaultState
    : defaultState;

export default(state = initialState, action) => {
    switch (action.type) {

        case {{lowerCase name}}Routine.SUCCESS:
            return Object.assign({}, state, { data: action.payload });

        default:
            return state;
    }
};
