        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "purgecss-webpack-plugin": "^1.5.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-helmet": "^5.2.1",
        "react-hot-loader": "^4.11.1",
        "react-icons": "^3.7.0",
        "react-intl": "^2.9.0",
