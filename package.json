{
    "name": "rrwbb",
    "version": "0.5.0",
    "author": "tohachan <toha.chan@cmail.com>",
    "description": "React Webpack Babel setup with SSR, HMR and Code splitting",
    "keywords": [
        "React",
        "Webpack",
        "Babel",
        "SSR",
        "HMR",
        "Code splitting",
        "Starter"
    ],
    "license": "MIT",
    "scripts": {
        "analyze": "npm run build && source-map-explorer build/public/main.bundle.*",
        "dev": "rimraf ./build && cross-env NODE_ENV=development node scripts/start-dev.js",
        "build": "rimraf ./build && npm run build:client && npm run build:server",
        "build:server": "webpack --config ./config/webpack/webpack.server.prod.config.js",
        "build:client": "webpack --config ./config/webpack/webpack.client.prod.config.js",
        "prod": "cross-env NODE_ENV=production npm run build && node build/bundle.js",
        "start:prod": "node build/bundle.js"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/node": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "7.2.3",
        "@babel/plugin-transform-runtime": "7.2.0",
        "@babel/preset-env": "7.2.3",
        "@babel/preset-react": "7.0.0",
        "@loadable/babel-plugin": "^5.10.0",
        "@loadable/webpack-plugin": "^5.7.1",
        "autoprefixer": "9.4.3",
        "babel-loader": "8.0.4",
        "babel-plugin-module-resolver": "^3.2.0",
        "css-loader": "2.1.0",
        "file-loader": "3.0.1",
        "html-webpack-plugin": "3.2.0",
        "node-sass": "4.11.0",
        "nodemon": "^1.19.1",
        "offline-plugin": "^5.0.7",
        "postcss-loader": "3.0.0",
        "sass-loader": "7.1.0",
        "source-map-explorer": "^2.0.0",
        "style-loader": "0.23.1",
        "webpack": "4.28.3",
        "webpack-cli": "3.1.2",
        "webpack-dev-middleware": "^3.7.0",
        "webpack-dev-server": "3.1.14",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-hot-server-middleware": "^0.6.0"
    },
    "dependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/runtime": "7.2.0",
