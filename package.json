{
    "name": "rrwbb",
    "version": "0.6.2",
    "author": "tohachan <toha.chan@gmail.com>",
    "description": "React Webpack Babel setup with SSR, HMR and Code splitting",
    "keywords": [
        "React",
        "Webpack",
        "Babel",
        "SSR",
        "HMR",
        "Code splitting",
        "Starter"
    ],
    "license": "MIT",
    "scripts": {
        "dev": "rimraf ./build && cross-env NODE_ENV=development node scripts/start-dev.js",
        "prod": "cross-env NODE_ENV=production npm run build && node build/bundle.js",
        "build": "rimraf ./build && npm run build:client && npm run build:server",
        "build:server": "webpack --config ./config/webpack/webpack.server.prod.config.js",
        "build:client": "webpack --config ./config/webpack/webpack.client.prod.config.js",
        "build:spa": "rimraf ./build && webpack --config ./config/webpack/webpack.client.spa.config.js",
        "start:prod": "node build/bundle.js",
        "start:spa": "node scripts/start-spa",
        "gen": "plop --plopfile generators/index.js",
        "analyze": "npm run build && source-map-explorer build/public/main.bundle.*",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "extract-intl": "node ./scripts/extract-intl.js"
    },
    "lint-staged": {
        "*.js": [
            "eslint",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "devDependencies": {
        "@babel/core": "7.5.5",
        "@babel/node": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "7.5.5",
        "@babel/plugin-transform-runtime": "7.5.5",
        "@babel/preset-env": "7.5.5",
        "@babel/preset-react": "7.0.0",
        "@loadable/babel-plugin": "^5.10.0",
        "@loadable/webpack-plugin": "^5.7.1",
        "autoprefixer": "9.6.1",
        "babel-eslint": "^10.0.2",
        "babel-loader": "8.0.6",
        "babel-plugin-module-resolver": "^3.2.0",
        "babel-plugin-react-intl": "^3.5.1",
        "chalk": "^2.4.2",
        "copy-webpack-plugin": "^5.0.4",
        "css-loader": "3.2.0",
        "eslint": "^6.2.0",
        "eslint-plugin-react": "^7.14.3",
        "file-loader": "4.2.0",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^3.0.4",
        "node-sass": "4.12.0",
        "nodemon": "^1.19.1",
        "offline-plugin": "^5.0.7",
        "plop": "^2.4.0",
        "postcss-loader": "3.0.0",
        "readline": "^1.3.0",
        "replace-in-file": "^4.1.3",
        "sass-loader": "7.2.0",
        "shelljs": "^0.8.3",
        "source-map-explorer": "^2.0.0",
        "style-loader": "1.0.0",
        "webpack": "4.39.2",
        "webpack-cli": "3.3.7",
        "webpack-dev-middleware": "^3.7.0",
        "webpack-dev-server": "^3.8.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-hot-server-middleware": "^0.6.0"
    },
    "dependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/runtime": "7.5.5",
        "@hot-loader/react-dom": "^16.9.0",
        "@loadable/component": "^5.10.2",
        "@loadable/server": "^5.10.2",
        "bulma": "^0.7.5",
        "classnames": "^2.2.6",
        "compression": "^1.7.4",
        "connected-react-router": "^6.5.2",
        "cookie-parser": "^1.4.4",
        "cross-env": "^5.2.0",
        "extract-css-chunks-webpack-plugin": "^4.6.0",
        "glob": "^7.1.4",
        "history": "^4.9.0",
        "intersection-observer": "^0.7.0",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.15",
        "node-fetch": "^3.1.1",
        "null-loader": "^3.0.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prop-types": "^15.7.2",
        "purgecss-webpack-plugin": "^1.5.0",
